  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'rust_9cc'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=rust_9cc"
        ],
        "filter": {
          "name": "rust_9cc",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'main'",
      "cargo": {
        "args": [
          "build",
          "--bin=main",
          "--package=rust_9cc"
        ],
        "filter": {
          "name": "main",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'main'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=main",
          "--package=rust_9cc"
        ],
        "filter": {
          "name": "main",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug integration test 'test'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--test=test",
          "--package=rust_9cc"
        ],
        "filter": {
          "name": "test",
          "kind": "test"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}